<div class="ts-main-content">
    <div>
        @Html.Partial("Header")
    </div>
    <div>
        @Html.Partial("HostelClerk_Sidebar")
    </div>
    <div class="content-wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <h2 class="page-title" style="margin-top: 5%">Students</h2>
                    <div class="panel panel-default">
                        <div class="panel-heading">All Students</div>
                        <div class="panel-body">
                            <table id="zctb" class="display table table-striped table-bordered table-hover" cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th>Student CNIC</th>
                                        <th>Student Name</th>
                                        <th>Allottment Status</th>
                                        <th>Recent Session</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                @foreach (var s in Model)
                                {
                                    <tr>
                                        <td>@s.std_cnic</td>
                                        <td>@s.std_name</td>
                                        <td>inactive</td>
                                        <td>@s.session_startDate - @s.session_endDate</td>
                                        <td><a href="/HostelClerk/Add/@s.std_id">Add Allottment</a></td>
                                    </tr>

                                }

                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
